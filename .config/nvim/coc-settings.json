{
  "python.venvPath": "${env:HOME}/.pyenv/versions",
    "python.formatting.provider": "black",
      "python.formatting.blackPath": "~/.pyenv/versions/coc/bin/black",
        "coc.preferences.formatOnSaveFiletypes": [
          "javascript",
          "json"
        ],
          "eslint.autoFixOnSave": true,
            "eslint.probe": [
              "javascript",
              "typescript"
            ]
}
